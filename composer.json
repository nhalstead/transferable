{
    "name": "nhalstead/transferable",
    "description": "Transfer attached relationships from one model instance to another model instance of the same type.",
    "keywords": [
        "laravel",
        "eloquent",
        "transerable"
    ],
    "homepage": "https://github.com/nhalstead/transferable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Noah Halstead",
            "email": "nhalstead00@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "~6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "nhalstead\\Transferable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nhalstead\\Transferable\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
